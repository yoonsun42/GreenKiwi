extends layout

block content
    // Slider
    .slider-container
      .container
        .row

    // Presentation
    .presentation-container
      .container
        .row
          .col-sm-12.wow.fadeInLeftBig
            h1
              span.violet GreenKiwi
              | 는 실시간 이슈 제공 서비스입니다.
            p 네이버 검색 API를 이용하여 실시간 이슈를 분석해 보여줍니다.
    // Services
    .services-container
      .container
        .row
          each val in arr
            include item.jade

    // Footer
    footer
      .container
        .row
          .col-sm-3.footer-box.wow.fadeInUp

          .col-sm-3.footer-box.wow.fadeInDown

          .col-sm-3.footer-box.wow.fadeInUp

          .col-sm-3.footer-box.wow.fadeInDown
            h4 Contact Us
            .footer-box-text.footer-box-text-contact
              p
                i.fa.fa-envelope
                | Jaejeon&nbsp:&nbsp
                a(href='#') jaejeon9@gmail.com
              p
                i.fa.fa-envelope
                | Yoonsun&nbsp:&nbsp
                a(href='#') smartys6964@gmail.com
        .row
          .col-sm-12.wow.fadeIn
            .footer-border
    // Javascript
    script(src='assets/js/jquery-1.11.1.min.js')
    script(src='assets/bootstrap/js/bootstrap.min.js')
    script(src='assets/js/bootstrap-hover-dropdown.min.js')
    script(src='assets/js/jquery.backstretch.min.js')
    script(src='assets/js/wow.min.js')
    script(src='assets/js/retina-1.1.0.min.js')
    script(src='assets/js/jquery.magnific-popup.min.js')
    script(src='assets/flexslider/jquery.flexslider-min.js')
    script(src='assets/js/jflickrfeed.min.js')
    script(src='assets/js/masonry.pkgd.min.js')
    script(src='http://maps.google.com/maps/api/js?sensor=true')
    script(src='assets/js/jquery.ui.map.min.js')
    script(src='assets/js/scripts.js')
